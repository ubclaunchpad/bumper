{
    "extends": "airbnb",
    "globals": {
        "window": false,
        "WebSocket": false,
        "document": false
    },
    "rules": {
        "react/jsx-filename-extension": 0,
        "react/prop-types": 0,
        "react/prefer-stateless-function": 0,
        "no-use-before-define": 0,
        "max-len": 0,
        "no-param-reassign": 1,
        "no-mixed-operators": 1
    }
}